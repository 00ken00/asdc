            "time": 48.000799894332886
        },
        {
            "id": {
                "id": "asdc.v4.148.2"
            },
            "name": "operator_2",
            "text": "かしこまりました。ご予約のお日にちを教えてください。",
            "text_sbs": [
                9,
                26
            ],
            "time": 84.00129985809326
        },
        {
            "id": {
                "id": "asdc.v4.148.3"
            },
            "name": "customer_25",
            "text": "1月1日より3泊でお願いします。",
