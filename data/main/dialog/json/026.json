            "time": 583.0158998966217
        },
        {
            "id": {
                "id": "asdc.v4.026.16"
            },
            "name": "operator_1",
            "text": "かしこまりました。それでは、ツインルームでよろしいでしょうか?",
            "text_sbs": [
                9,
                31
            ],
            "time": 600.0164999961853
        },
        {
            "id": {
                "id": "asdc.v4.026.17"
            },
            "name": "customer_5",
            "text": "はい。",
            "text_sbs": [
                3
            ],
            "time": 610.0167000293732
        },
        {
            "id": {
                "id": "asdc.v4.026.18"
