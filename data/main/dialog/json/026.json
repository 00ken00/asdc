    "utterances": [
        {
            "id": {
                "id": "asdc.v4.026.0"
            },
            "name": "operator_1",
            "text": "いらっしゃいませ。ご利用いただきまして、ありがとうございます。\nご宿泊先につきまして、お客様のご希望をお聞かせいただけますでしょうか。",
            "text_sbs": [
                9,
                32,
                67
